<li class="SortableRow no-nesting" id="ele-%%GLOBAL_List%%">
	<table cellpadding="0" cellspacing="0" width="100%" class="Text" style="margin:0; padding:0;">
		<tr class="GridRow">
			<td width="28" align="center">
				<input type="checkbox" name="Lists[]" value="%%GLOBAL_List%%" class="UICheckboxToggleRows">
			</td>
			<td width="22" class="DragMouseDown sort-handle">
				<img src="images/m_lists.gif" />
			</td>
			<td width="*" class="DragMouseDown sort-handle">
				%%GLOBAL_Name%%
			</td>
			<td width="120" class="HideOnDrag">
				%%GLOBAL_Created%%
			</td>
			<td width="120" class="HideOnDrag">
				%%GLOBAL_SubscriberCount%%
			</td>
			<td width="120" class="HideOnDrag">
				%%GLOBAL_Fullname%%
			</td>
			<td width="40" align="center" class="HideOnDrag">
				<a href="../rss.php?List=%%GLOBAL_List%%" target="_blank"><img src="images/rss.gif" border="0" title="%%LNG_RSS_Tip%%"></a>
			</td>
			<td width="240" class="HideOnDrag">
				%%GLOBAL_ListAction%%
			</td>
		</tr>
	</table>
</li>
